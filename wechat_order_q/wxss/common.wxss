/*icon图标  */

@font-face {
  font-family: 'iconfont';
  src: url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
         url('iconfont.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-minus2:before {
  content: "\e615";
}

.icon-plus1:before {
  content: "\e616";
}

/*公共头部  */

.posi_fiexd {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 100;
}

/*店铺图片圆角  */

.bd_radius {
  border-radius: 50%;
}
